  "scripts": {
    "start": "node src/server/index.js",
    "dev": "npm run setupConsul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "test": "npm run clean ; npm run setupConsul ; ${NODE_PATH}/.bin/nyc ${NODE_PATH}/.bin/mocha --recursive -R mocha-junit-reporter",
    "clean": "${NODE_PATH}/.bin/rimraf .nyc_output coverage test-resulsts.xml",
    "sc-db": "curl -X PUT -d ${MYSQL_DATABASE} http://consul:8500/v1/kv/einvoice-send/db-init/database",
    "sc-dbuser": "curl -X PUT -d 'root' http://consul:8500/v1/kv/einvoice-send/db-init/user",
    "sc-dbpw": "curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://consul:8500/v1/kv/einvoice-send/db-init/password",
    "sc-testdata": "curl -X PUT -d 'true' http://consul:8500/v1/kv/einvoice-send/db-init/populate-test-data",
    "sc-redispwd": "curl -X PUT -d ${REDIS_AUTH} http://consul:8500/v1/kv/einvoice-send/redis/password",
    "sc-scuser": "curl -X PUT -d 'svc_einvoice-send' http://consul:8500/v1/kv/einvoice-send/service-client/username",
    "sc-scpw": "curl -X PUT -d 'test' http://consul:8500/v1/kv/einvoice-send/service-client/password",
    "sc-sckey": "curl -X PUT -d 'oidcCLIENT' http://consul:8500/v1/kv/einvoice-send/service-client/client-key",
    "sc-oidcsecret": "curl -X PUT -d '91c0fabd17a9db3cfe53f28a10728e39b7724e234ecd78dba1fb05b909fb4ed98c476afc50a634d52808ad3cb2ea744bc8c3b45b7149ec459b5c416a6e8db242' http://consul:8500/v1/kv/einvoice-send/oauth-clients/oidcCLIENT",
    "sc-clientsecret": "curl -X PUT -d '91c0fabd17a9db3cfe53f28a10728e39b7724e234ecd78dba1fb05b909fb4ed98c476afc50a634d52808ad3cb2ea744bc8c3b45b7149ec459b5c416a6e8db242' http://consul:8500/v1/kv/einvoice-send/service-client/client-secret",
    "sc-redisauth": "curl -X PUT -d ${REDIS_AUTH} http://consul:8500/v1/kv/auth/redis/password",
    "setupConsul": "npm run sc-db & npm run sc-dbuser & npm run sc-dbpw & npm run sc-testdata & npm run sc-redispwd & npm run sc-scuser & npm run sc-scpw & npm run sc-sckey & npm run sc-oidcsecret & npm run sc-clientsecret & npm run sc-redisauth",
    "rest-doc": "mkdir -p wiki/rest-doc ; ${NODE_PATH}/.bin/raml2md -o file-per-resource -p wiki/rest-doc --output-ext=.endpoint.md rest-doc/main.raml",
    "domain-doc": "mkdir -p wiki/domain-doc ; ${NODE_PATH}/.bin/sq2md -o file-per-src -p wiki/domain-doc --init=init --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "api-doc": "mkdir -p wiki/api-doc ; ${NODE_PATH}/.bin/jsdoc2md -f $(find src/server) > wiki/api-doc/Home.md",
    "doc": "npm run rest-doc ; npm run domain-doc ",
    "build": "npm run build:client",
    "build:client": "webpack -p --config ./webpack.production.config.js --env=production --profile --colors --bail"
  }
}
