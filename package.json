    "nodemon": "^1.12.1",
    "nyc": "^10.3.2",
    "raml-to-markdown": "^1.1.7",
    "rimraf": "^2.6.2",
    "sequelize-to-markdown": "^1.1.13",
    "sqlite3": "^3.1.13",
    "style-loader": "^0.16.1",
    "webpack": "^2.6.1",
    "webpack-config": "^7.5.0",
